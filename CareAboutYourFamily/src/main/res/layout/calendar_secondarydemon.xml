<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/calendar_edit_padding">

    <TextView
        android:id="@+id/calendar_secondarydemon_t001"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="2"
        android:gravity="center_vertical"
        android:text="@string/calendar_secondarydemon_t001"
        android:textColor="@color/Black"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/calendar_secondarydemon_t002"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_weight="6"
        android:text="@string/calendar_secondarydemon_t002"
        android:textColor="@color/Black"
        android:textSize="18sp" />

</LinearLayout>